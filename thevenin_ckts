#!/usr/bin/perl -w
# vim:syntax=perl:
#
#
#@E12 = [1.0, 1.5, 2.2, 3.3, 4.7, 6.8];
@E6 = (1.0, 1.5, 2.2, 3.3, 4.7, 6.8);
@E12 = ( 1.0, 1.2, 1.5, 1.8, 2.2, 2.7, 3.3, 3.9, 4.7, 5.6, 6.8, 8.2);
@E24 = (1.0, 1.1, 1.2, 1.3, 1.5, 1.6, 1.8, 2.0, 2.2, 2.4, 2.7, 3.0, 3.3, 3.6, 3.9, 4.3, 4.7, 5.1, 5.6, 6.2, 6.8, 7.5, 8.2, 9.1);
@RVAL = @E12;
#
foreach $R1 (@RVAL) {
  foreach $R2 (@RVAL) {
    foreach $R3 (@RVAL) {
      $REQ = ($R1*$R2)/($R1+$R2) + $R3;
      foreach $RX (@RVAL) {
        if (abs($REQ-$RX)/$RX < 0.005) {
          print "R1=",$R1,"  R2=",$R2,"  R3=",$R3,"  REQ=",$REQ,"  RX=",$RX,"\n";
        }
      }
    }
  }
}
